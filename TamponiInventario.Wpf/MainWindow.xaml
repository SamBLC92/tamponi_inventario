<Window x:Class="TamponiInventario.Wpf.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:views="clr-namespace:TamponiInventario.Wpf.Views"
        xmlns:adminViews="clr-namespace:TamponiInventario.Wpf.Views.Admin"
        Title="Tamponi Inventario" Height="720" Width="1100">
    <Grid>
        <TabControl>
            <TabItem Header="Tamponi">
                <views:SwabsView />
            </TabItem>
            <TabItem Header="Scan">
                <views:ScanView />
            </TabItem>
            <TabItem Header="Storico">
                <views:HistoryView />
            </TabItem>
            <TabItem Header="Admin">
                <adminViews:AdminView />
            </TabItem>
        </TabControl>
    </Grid>
</Window>
